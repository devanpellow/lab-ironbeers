{{#each beers}}
<div class="random-card">
  <div class="random-img">
    <img src="{{this.image_url}}" alt="">
  </div>
  <div class="random-info">
    <h1>{{this.name}}</h1>
    <h2>{{this.tagline}}</h2>
    <h3>{{this.description}}</h3>

    <h4>Food Pairing</h4>
    <ul>
      {{#each this.food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
  </div>
</div>
{{/each}}